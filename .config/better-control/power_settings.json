{
  "lock": true,
  "logout": true,
  "suspend": true,
  "hibernate": false,
  "reboot": true,
  "shutdown": true,
  "commands": {
    "lock": "swaylock",
    "logout": "loginctl terminate-user $USER",
    "suspend": "systemctl suspend",
    "hibernate": "systemctl hibernate",
    "reboot": "reboot",
    "shutdown": "shutdown -h now"
  },
  "colors": {
    "lock": "#4A90D9",
    "logout": "#729FCF",
    "suspend": "#8DB67A",
    "hibernate": "#AD7FA8",
    "reboot": "#F8C146",
    "shutdown": "#ef5350"
  },
  "shortcuts": {
    "lock": "l",
    "logout": "o",
    "suspend": "s",
    "hibernate": "h",
    "reboot": "r",
    "shutdown": "p"
  },
  "show_keybinds": true
}